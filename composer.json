{
  "name": "middev/katas",
  "type": "project",
  "require": {
    "php": "^8.3"
  },
  "require-dev": {
    "pestphp/pest": "^4.2",
    "phpunit/phpunit": "^12.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
